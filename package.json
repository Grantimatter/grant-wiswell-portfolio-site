{
  "name": "grant-wiswell-portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "storybook test"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@playwright/test": "^1.28.1",
    "@skeletonlabs/skeleton": "^0.80.9",
    "@storybook/addon-actions": "^7.0.0-beta.13",
    "@storybook/addon-coverage": "^0.0.7",
    "@storybook/addon-essentials": "^7.0.0-beta.13",
    "@storybook/addon-interactions": "^7.0.0-beta.13",
    "@storybook/addon-links": "^7.0.0-beta.13",
    "@storybook/svelte": "^7.0.0-beta.13",
    "@storybook/sveltekit": "^7.0.0-beta.13",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/svelte": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/coverage-c8": "^0.26.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.20",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.13",
    "svelte": "^3.55.0",
    "svelte-check": "^2.9.2",
    "svelte-loader": "^3.1.4",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
